{
	"compilerOptions": {
		"target": "ES2020",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"lib": [
			"es6"
		],
		"allowJs": true,
		"outDir": "dist",
		"rootDir": "src",
		"strict": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"baseUrl": "./src",
		"paths": {
		  "@/*": [
			"*"
		  ]
		}
	},
	"include": [
		"src"
	],
	"exclude": []
}